<%= form_with url: "/", method: :get, local: true do |form| %>
  <%= form.text_field :search %>
  <%= form.submit "検索"%>
<% end %>

<% (0..9).each do |number| %>
<div class="card">
    <div class="card-header">
        <a href=<%=@response_data["response"]["#{number}"]["photo"]["shop_url"]%>><%= @response_data["response"]["#{number}"]["photo"]["shop_name"]%></a>
    </div>
    <div class="card-body">
        <%= @response_data["response"]["#{number}"]["photo"]["menu_name"]%>
        <img src="<%= @response_data["response"]["#{number}"]["photo"]["image_url"]["url_320"]%>" alt="画像">
        <%= @response_data["response"]["#{number}"]["photo"]["comment"]%>
        <%= @response_data["response"]["#{number}"]["photo"]["update_date"]%>

    </div>
</div>
<% end %>